scalar GraphQLDateType

type Tweet {
    id: ID!
    author: User!
    text: String!
    likes: Int
}

type Query {
    tweet(id: ID!): Tweet
    allTweets: [Tweet]!
    countTweets: Long!
    tweetsByAuthor(authorId: ID): [Tweet]
}

type Mutation {
    newTweet(authorId: Long, text: String) : Tweet!
    deleteTweet(id: Long!) : Boolean
    updateTweetLikes(likes: Int, id: Long) : Tweet!
}