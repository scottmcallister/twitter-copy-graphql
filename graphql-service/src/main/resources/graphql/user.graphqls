type User {
    id: ID!
    name: String!
    handle: String!
    email: String!
    password: String
}

extend type Query {
    findAllUsers: [User]!
    countUsers: Long!
    findUserByHandle(handle: String): User
}

extend type Mutation {
    newUser(name: String!, handle: String!, email: String!, password: String!) : User!
}
